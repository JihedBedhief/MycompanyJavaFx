<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="gui.AbonnementController">
    <stylesheets>
        <URL value="@abonnement.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tab" layoutX="252.0" layoutY="67.0" prefHeight="200.0" prefWidth="306.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="75.0" text="Id" />
            <TableColumn fx:id="type" prefWidth="75.0" text="Type" />
            <TableColumn fx:id="dur" prefWidth="75.0" text="durée" />
            <TableColumn fx:id="frais" prefWidth="75.0" text="Frais" />
         </columns>
      </TableView>
      <TextField fx:id="t" layoutX="55.0" layoutY="124.0" promptText="type" />
      <TextField fx:id="dure" layoutX="55.0" layoutY="174.0" promptText="durée" />
      <Button layoutX="55.0" layoutY="287.0" mnemonicParsing="false" onAction="#AddDivision" text="Ajout" />
      <Button layoutX="111.0" layoutY="287.0" mnemonicParsing="false" onAction="#Modifier" text="Modifier" />
      <Button layoutX="181.0" layoutY="287.0" mnemonicParsing="false" onAction="#Supprimer" text="Supprimer" />
      <TextField fx:id="idx" layoutX="55.0" layoutY="87.0" />
      <TextField fx:id="F" layoutX="55.0" layoutY="214.0" promptText="frais" />
   </children>
</AnchorPane>
